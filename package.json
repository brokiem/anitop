{
  "name": "anitop",
  "version": "1.0.0",
  "description": "Anitop is an unofficial simple API that provides a data trending anime from Anitrendz website",
  "main": "index.js",
  "scripts": {
    "scrape:music-chart": "ts-node ./src/musicChart.ts",
    "scrape:couple": "ts-node ./src/couple.ts",
    "scrape:male": "ts-node ./src/maleCharacters.ts",
    "scrape:female": "ts-node ./src/femaleCharacters.ts",
    "scrape:opening": "ts-node ./src/openingSongs.ts",
    "scrape:ending": "ts-node ./src/endingSongs.ts",
    "scrape:top-anime": "ts-node ./src/topAnime.ts"
  },
  "keywords": [
    "anime",
    "scrapper",
    "typescript",
    "anitrendz",
    "serverless",
    "vercel"
  ],
  "author": "Satya Wikananda",
  "license": "MIT",
  "devDependencies": {
    "@types/cheerio": "^0.22.23",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@vercel/node": "^1.8.5",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.3",
    "chrome-aws-lambda": "10.1.0",
    "puppeteer-core": "10.1.0"
  }
}
